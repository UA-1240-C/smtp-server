{
    "version": 3,
    "configurePresets": [
        {   
            "name": "default",
            "description": "Defalut settings for my cmake project",
            "hidden": false,
            "generator": "Unix Makefiles",
            "binaryDir": "${workspaceFolder}/build",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKe_EXPORT_COMPILE_COMMANDS": "YES"
            }
        },
        {   
            "name": "release",
            "description": "Release settings for my cmake project",
            "inherits": "default",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release"
            }
        }

    ]
}